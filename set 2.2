//find the count invertion of  array elements

void invertion(int a[],int n)
{
	int count =0;
	for(int i=0;i<n;i++)
	{
		for(int j=i+1;j<n;j++)
		{
			if(a[i]>a[j])
			{
				cout<<"("<<a[i]<<","<<a[j]<<")"<<endl;
				count++;
			}
		}
	}
	cout<<"invertions"<<count;
}
int main()
{
	int n;
	cin>>n;
	int a[n];
	cout<<"enter a elemnt of an array:";
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
	}
	invertion(a,n);
}
